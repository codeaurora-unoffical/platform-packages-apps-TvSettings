                    android:id="@+id/frame2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <FrameLayout
                    android:id="@+id/frame2_overlay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/tp_preview_panel_overlay_color"
                    android:alpha="0"
                    android:focusable="false"/>
