                android:layout_width="@dimen/tp_settings_preference_pane_width"
                android:layout_height="match_parent" >
                <FrameLayout
                    android:id="@+id/frame3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <FrameLayout
